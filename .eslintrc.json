{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "standard",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": "off",
        "no-tabs": "off",
        "no-unused-vars": ["warn"],
        "comma-dangle": ["error", "never"],
        "space-before-function-paren": ["error", {"asyncArrow": "always", "anonymous": "always", "named": "never"}]
    },
    "ignorePatterns": ["src/slashCommands.js"]
}
